{
  "extends": "./tsconfig.app.json",
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.vitest.tsbuildinfo",
    "lib": ["DOM", "DOM.Iterable", "ES2020"],
    "types": ["node", "vitest/globals"]
  },
  "include": [
    "env.d.ts",
    "src/**/*.ts",
    "src/**/*.vue",
    "src/**/*.tsx",
    "src/**/__tests__/**/*.ts",
    "src/**/__tests__/**/*.tsx",
    "vitest.setup.ts",
    "vitest.config.ts"
  ],
  "exclude": []
}
